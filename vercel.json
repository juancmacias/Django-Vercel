{
  "version": 2,
  "builds": [
    {
      "src": "myproyect/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)/", "dest": "myproyect/wsgi.py" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}